{
  "xsappname": "training",
  "tenant-mode": "dedicated",
  "scopes": [
    {
      "name": "$XSAPPNAME.User",
      "description": "User"
    },
    {
      "name": "$XSAPPNAME.Admin",
      "description": "Administrator"
    }
  ],
  "role-templates": [
    {
      "name": "User",
      "description": "User Role Template",
      "scope-references": [
        "$XSAPPNAME.User"
      ]
    },
    {
      "name": "Admin",
      "description": "Administrator Role Template",
      "scope-references": [
        "$XSAPPNAME.Admin"
      ]
    }
  ],
  "role-collections": [
    {
      "name": "training_User",
      "description": "Training User",
      "role-template-references": [
        "$XSAPPNAME.User"
      ]
    },
    {
      "name": "training_Admin",
      "description": "Training Administrator",
      "role-template-references": [
        "$XSAPPNAME.Admin"
      ]
    }
  ],
  "attributes": [],
  "oauth2-configuration": {
    "token-validity": 3600,
    "redirect-uris": [
      "https://*.cfapps.eu10.hana.ondemand.com/**"
    ]
  }
}
